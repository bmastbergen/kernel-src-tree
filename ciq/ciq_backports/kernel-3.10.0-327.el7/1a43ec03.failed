+ #define	XFS_SB_UUID		XFS_SB_MVAL(UUID)
+ #define	XFS_SB_FNAME		XFS_SB_MVAL(FNAME)
+ #define	XFS_SB_ROOTINO		XFS_SB_MVAL(ROOTINO)
