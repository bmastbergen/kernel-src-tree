+ /*
+  * define max. shared version we can interoperate with
+  */
+ #define XFS_SB_MAX_SHARED_VN	0
+ 
+ #define	XFS_SB_VERSION_NUM(sbp)	((sbp)->sb_versionnum & XFS_SB_VERSION_NUMBITS)
