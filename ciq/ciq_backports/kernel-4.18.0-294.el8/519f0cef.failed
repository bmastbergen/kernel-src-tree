+ static int hw_atl_b0_extts_gpio_enable(struct aq_hw_s *self, u32 index,
+ 				       u32 enable)
+ {
+ 	/* Enable/disable Sync1588 GPIO Timestamping */
+ 	aq_phy_write_reg(self, MDIO_MMD_PCS, 0xc611, enable ? 0x71 : 0);
+ 
+ 	return 0;
+ }
