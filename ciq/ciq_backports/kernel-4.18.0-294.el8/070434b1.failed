+ /*
+  * Returns true for a R -> RW upgrade of pte
+  */
+ static inline bool is_pte_rw_upgrade(unsigned long old_val, unsigned long new_val)
+ {
+ 	if (!(old_val & _PAGE_READ))
+ 		return false;
+ 
+ 	if ((!(old_val & _PAGE_WRITE)) && (new_val & _PAGE_WRITE))
+ 		return true;
+ 
+ 	return false;
+ }
