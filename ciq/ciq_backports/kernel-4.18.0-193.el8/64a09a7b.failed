+ static void xfrm_hash_reset_inexact_table(struct net *net)
+ {
+ 	struct xfrm_pol_inexact_bin *b;
+ 
+ 	lockdep_assert_held(&net->xfrm.xfrm_policy_lock);
+ 
+ 	list_for_each_entry(b, &net->xfrm.inexact_bins, inexact_bins)
