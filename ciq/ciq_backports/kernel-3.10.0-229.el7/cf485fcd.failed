 +	pages = data->pages.pagevec;
 +	while (!list_empty(head)) {
 +		req = nfs_list_entry(head->next);
 +		nfs_list_remove_request(req);
 +		nfs_list_add_request(req, &hdr->pages);
 +		*pages++ = req->wb_page;
