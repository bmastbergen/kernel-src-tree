+ 	if (desc->pg_bsize < PAGE_CACHE_SIZE)
+ 		return nfs_pgio_multi(desc, hdr);
+ 	return nfs_pgio_one(desc, hdr);
+ }
+ EXPORT_SYMBOL_GPL(nfs_generic_pgio);
