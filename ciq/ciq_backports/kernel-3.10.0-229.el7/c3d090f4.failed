+ ifeq ($(plugin_dir),)
+ ifeq ($(prefix),$(HOME))
+ override plugin_dir = $(HOME)/.traceevent/plugins
+ set_plugin_dir := 0
+ else
+ override plugin_dir = $(prefix)/lib/traceevent/plugins
+ endif
