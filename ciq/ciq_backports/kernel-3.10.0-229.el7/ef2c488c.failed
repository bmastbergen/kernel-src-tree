 +		nfs_list_remove_request(req);
 +		nfs_list_add_request(req, &hdr->pages);
 +		*pages++ = req->wb_page;
 +	}
 +
 +	nfs_read_rpcsetup(data, desc->pg_count, 0);
 +	list_add(&data->list, &hdr->rpc_list);
 +	desc->pg_rpc_callops = &nfs_read_common_ops;
 +	return 0;
 +}
