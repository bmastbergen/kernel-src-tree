+ 						     BRIDGE_VLAN_INFO_UNTAGGED);
+ 			if (!err && vlan->flags & BRIDGE_VLAN_INFO_PVID)
+ 				err = ds->drv->port_pvid_set(ds, p->port, vid);
+ 			if (err)
+ 				return err;
+ 		}
+ 	}
+ 
+ 	return 0;
+ }
