+ static inline netdev_tx_t dsa_netpoll_send_skb(struct dsa_slave_priv *p,
+ 					       struct sk_buff *skb)
+ {
+ #ifdef CONFIG_NET_POLL_CONTROLLER
+ 	if (p->netpoll)
