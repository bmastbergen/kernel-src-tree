+ 	pdata->kr_state = XGBE_RX_BPA;
+ 	pdata->kx_state = XGBE_RX_BPA;
+ 
+ 	/* Re-enable auto-negotiation interrupt */
+ 	enable_irq(pdata->an_irq);
+ 
+ 	xgbe_an_init(pdata);
+ 	xgbe_an_restart(pdata);
+ 
+ 	return 0;
