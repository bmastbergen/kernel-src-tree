+ {
+ 	/* 10us base */
+ 	if (vc->halt_poll_ns == 0 && halt_poll_ns_grow)
+ 		vc->halt_poll_ns = 10000;
+ 	else
+ 		vc->halt_poll_ns *= halt_poll_ns_grow;
+ }
