+ static void shrink_halt_poll_ns(struct kvmppc_vcore *vc)
+ {
+ 	if (halt_poll_ns_shrink == 0)
+ 		vc->halt_poll_ns = 0;
+ 	else
+ 		vc->halt_poll_ns /= halt_poll_ns_shrink;
