+ 				  RING_FORCE_TO_NONPRIV_RANGE_4);
+ 
+ 		/* Wa_1808121037:tgl */
+ 		whitelist_reg(w, GEN7_COMMON_SLICE_CHICKEN1);
+ 		break;
+ 	default:
+ 		break;
+ 	}
+ }
