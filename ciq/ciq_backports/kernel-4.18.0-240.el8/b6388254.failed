+ 
+ 	if (IS_ERR(skb)) {
+ 		err = PTR_ERR(skb);
+ 		bt_dev_err(hdev, "QCA preshutdown_cmd failed (%d)", err);
+ 		return err;
+ 	}
+ 
+ 	kfree_skb(skb);
+ 
+ 	return 0;
+ }
+ EXPORT_SYMBOL_GPL(qca_send_pre_shutdown_cmd);
