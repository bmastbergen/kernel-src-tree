+ int qca_send_pre_shutdown_cmd(struct hci_dev *hdev)
+ {
+ 	struct sk_buff *skb;
+ 	int err;
+ 
+ 	bt_dev_dbg(hdev, "QCA pre shutdown cmd");
+ 
+ 	skb = __hci_cmd_sync_ev(hdev, QCA_PRE_SHUTDOWN_CMD, 0,
