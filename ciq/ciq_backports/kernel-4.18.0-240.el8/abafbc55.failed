+ void vfio_pci_zap_and_down_write_memory_lock(struct vfio_pci_device *vdev)
+ {
+ 	vfio_pci_zap_and_vma_lock(vdev, false);
+ 	down_write(&vdev->memory_lock);
+ 	mutex_unlock(&vdev->vma_lock);
+ }
