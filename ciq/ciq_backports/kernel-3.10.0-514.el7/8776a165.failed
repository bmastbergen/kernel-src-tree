+ #define PCI_DEVICE_ID_INTEL_PCH_SDIO0	0x8809
+ #define PCI_DEVICE_ID_INTEL_PCH_SDIO1	0x880a
+ #define PCI_DEVICE_ID_INTEL_BYT_EMMC	0x0f14
