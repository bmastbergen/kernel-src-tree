+ static void dso__adjust_kmod_long_name(struct dso *dso, const char *filename)
+ {
+ 	const char *dup_filename;
+ 
+ 	if (!filename || !dso || !dso->long_name)
+ 		return;
+ 	if (dso->long_name[0] != '[')
+ 		return;
+ 	if (!strchr(filename, '/'))
+ 		return;
+ 
+ 	dup_filename = strdup(filename);
+ 	if (!dup_filename)
+ 		return;
+ 
+ 	dso__set_long_name(dso, filename, true);
+ }
