+ BPF_CALL_2(bpf_xdp_adjust_head, struct xdp_buff *, xdp, int, offset)
+ {
+ 	void *data = xdp->data + offset;
+ 
+ 	if (unlikely(data < xdp->data_hard_start ||
+ 		     data > xdp->data_end - ETH_HLEN))
+ 		return -EINVAL;
+ 
+ 	xdp->data = data;
+ 
+ 	return 0;
+ }
